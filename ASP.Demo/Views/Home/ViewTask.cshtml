@{
    Layout = "";
    bool haveData = true;
}

@for(int i = 0; i < 20 && haveData; i++){
    @if (TempData.ContainsKey($"Task{i}"))
    {
        <p>@TempData[$"Task{i}"]</p>
    }
    else
    {
        haveData = false;
    }
}
